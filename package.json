{
  "name": "@autoschedule/scheduler-tester",
  "version": "0.0.0",
  "description": "Tool to test queries-scheduler",
  "main": "dist/index.bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ava": "ava **/*.test.js",
    "test": "tslint -p ./ && tsc && nyc --reporter=lcov npm run ava",
    "test:codecov": "tslint -p ./ && tsc && nyc npm run ava",
    "build": "webpack",
    "doc": "npm run build && typedoc --out docs/ dist/",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "start": "parcel src/index.html",
    "watch:ts": "tsc -w",
    "watch:ava": "ava -w build/**/*.test.js",
    "watch:test": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Ava\" -c \"blue.bold,magenta.bold\" \"npm run watch:ts\" \"npm run watch:ava\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AutoScheduleJS/scheduler-tester.git"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.13",
    "vue-rx": "^5.0.0"
  },
  "devDependencies": {
    "@autoschedule/queries-fn": "^4.1.1",
    "@autoschedule/queries-scheduler": "^3.1.0",
    "@autoschedule/userstate-manager": "^2.0.1",
    "@types/ramda": "github:types/npm-ramda#dist",
    "ava": "^0.25.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "emotion": "^9.0.0",
    "nyc": "^11.4.1",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.9.2",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.6",
    "ts-loader": "^3.3.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.6.2",
    "typescript-styled-plugin": "^0.4.0"
  }
}
