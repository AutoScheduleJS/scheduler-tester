{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": [
      "esnext",
      "dom"
    ],
    "plugins": [{
      "name": "typescript-styled-plugin",
      "tags": [
        "css",
        "keyframes"
      ]
    }],
    "types": [],
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    "declaration": false,
    "inlineSourceMap": false,
    "inlineSources": false,
    "outDir": "./build",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": false,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@scheduler-tester/core-state/*": ["src/core-state/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
